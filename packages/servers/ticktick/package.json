{
  "name": "@mcp-server/ticktick",
  "version": "0.1.0",
  "description": "MCP Server for TickTick todo-list application",
  "keywords": [
    "mcp",
    "ticktick",
    "todo",
    "task-management",
    "api",
    "claude"
  ],
  "license": "MIT",
  "author": "Fractalizer",
  "type": "module",
  "imports": {
    "#config": "./src/config/index.ts",
    "#config/*": "./src/config/*",
    "#ticktick_api/*": "./src/ticktick_api/*",
    "#tools/*": "./src/tools/*",
    "#composition-root/*": "./src/composition-root/*",
    "#cli/*": "./src/cli/*",
    "#constants": "./src/constants.ts",
    "#common/*": "./src/common/*",
    "#integration/*": "./tests/integration/*",
    "#helpers/*": "./tests/helpers/*"
  },
  "main": "./dist/index.js",
  "bin": {
    "mcp-server-ticktick": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "tsc -b && tsc-alias",
    "clean": "rimraf dist",
    "cpd": "jscpd src || true",
    "cpd:quiet": "jscpd src --silent || true",
    "cpd:report": "jscpd src --reporters html,console",
    "depcruise": "depcruise src --validate ../../../.dependency-cruiser.cjs || true",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:quiet": "eslint src --ext .ts --quiet",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:quiet": "vitest run --reporter=dot --silent",
    "test:smoke": "echo 'Smoke tests not implemented yet'",
    "test:verbose": "vitest run --reporter=verbose",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test && npm run cpd",
    "validate:quiet": "npm run lint:quiet && npm run typecheck && npm run test:quiet && npm run cpd:quiet"
  },
  "dependencies": {
    "@mcp-framework/cli": "^0.2.0",
    "@mcp-framework/core": "^0.1.0",
    "@mcp-framework/infrastructure": "^0.2.0",
    "@mcp-framework/search": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "inversify": "^7.10.4",
    "pino": "^10.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "axios-mock-adapter": "^2.1.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
